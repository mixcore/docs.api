{
  "metadata": [
    {
      "src": [
        {
          "src": "../../",
          "files": [
            "mix.core/src/MixCore.sln"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "**/_site/**",
            "**/docs/**"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "docfx.plugins.plantuml/1.1.24/template"
    ],
    "postProcessors": [],
    "markdownEngineProperties": {
        "plantUml.localPlantUmlPath": "plantuml.jar",
        "plantUml.outputFormat":"svg",
        "plantUml.javaPath": "C:/Program Files/Java/jre1.8.0_271",
        "plantUml.localGraphvizDotPath": "C:/Program Files/Graphviz/bin/dot.exe",
        "plantUml.remoteUrl":"http://www.plantuml.com/plantuml/",
        "plantUml.renderingMode":"remote"
    },
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": true,
    "disableGitFeatures": false
  }
}