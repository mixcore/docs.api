@startuml
class MixMessengerMessage <<partial>> {
    + Id : Guid <<get>> <<set>>
    + Content : string <<get>> <<set>>
    + CreatedDate : DateTime <<get>> <<set>>
    + RoomId : Guid? <<get>> <<set>>
    + TeamId : int? <<get>> <<set>>
    + UserId : string <<get>> <<set>>
    + Room : MixMessengerHubRoom <<get>> <<set>>
    + Team : MixMessengerTeam <<get>> <<set>>
    + User : MixMessengerUser <<get>> <<set>>
}
@enduml
